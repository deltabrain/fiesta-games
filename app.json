{
	"expo": {
		"name": "Fiesta",
		"slug": "Fiesta-Trinkspiele",
		"owner": "deltabrain",
		"version": "0.2.5",
		"orientation": "portrait",
		"icon": "./src/assets/images/logo.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./src/assets/images/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#151718"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "io.deltabrain.fiesta",
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/images/logo.png",
				"backgroundColor": "#151718"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-image-picker",
			[
				"expo-font",
				{
					"fonts": [
						"./node_modules/@expo-google-fonts/comfortaa/Comfortaa_500Medium.ttf"
					]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "7f14d39f-e934-4e03-865c-45a205720657"
			}
		}
	}
}
