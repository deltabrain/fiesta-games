{
  "expo": {
    "name": "Fiesta",
    "slug": "Fiesta-Trinkspiele",
    "owner": "deltabrain",
    "version": "0.2.4",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#151718"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.deltabrain.fiesta"
    },
    "android": {
      "package": "io.deltabrain.fiesta",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#151718"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7f14d39f-e934-4e03-865c-45a205720657"
      }
    }
  }
}
